<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.itwill.spring2.repository.MenuRepository">

<select id="findByRestaurantId" resultType="com.itwill.spring2.dto.MenuDto">
    SELECT * FROM Menus
     WHERE restaurant_id = #{id}
</select>
</mapper>